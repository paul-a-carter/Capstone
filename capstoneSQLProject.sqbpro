FROM &quot;nflWRStats2000-2022&quot;
WHERE Pos = &quot;WR&quot;
GROUP BY Player
HAVING totalYds &gt; 500
ORDER BY YPG DESC

SELECT Player, SUM(Yds) AS totalYds, SUM(Yds)/SUM(G) AS YPG, nflCombineDataFull.&quot;Height (in)&quot;, nflCombineDataFull.&quot;Arm Length (in)&quot;, nflCombineDataFull.&quot;40 Yard&quot;
FROM fullNFLStats
LEFT JOIN nflCombineDataFull
ON fullNFLStats.Player = nflCombineDataFull.Name
WHERE fullNFLStats.Pos = &quot;WR&quot;
GROUP BY Player
HAVING SUM(G) &gt;= 17 AND nflCombineDataFull.Year &gt; 1995 AND totalYds &gt;= 1000
ORDER BY YPG DESC</sql><current_tab id="0"/></tab_sql></sqlb_project>
